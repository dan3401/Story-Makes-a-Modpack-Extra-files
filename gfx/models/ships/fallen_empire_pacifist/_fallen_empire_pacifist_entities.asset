	state = { name = "moving" animation = "closing" looping = no next_state = "moving"
		chance = {
			if_current_state = {
				"idle" = 0
				"moving" = 0
				"death" = 0
				"working" = 0
				"working_looping" = 100
			}
		}
		event = { trigger_once = yes sound = { soundeffect = "fallen_empire_colossus_closing" stop_on_state_change = yes } }
		event = { time = 0 node = "light_locator_01" particle = "ship_light_violet_effect" keep_particle = yes trigger_once = yes }
