		event = { time = 0 node = "planet_killer_gun_01" particle = "magenta_colossus_core_effect" keep_particle = yes trigger_once = yes }
	}
	# Closed death state.
	state = { name = "death" animation = "idle" looping = no
		chance = {
			if_current_state = {
				"idle" = 100
				"moving" = 100
				"death" = 0
